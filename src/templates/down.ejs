                    <img src="./imgs/pdf2.jpg" alt="" />
                  </figure>
                  <!--<div class="down-pdf">PDF</div>-->
                </div>
                <div class="section-top-30 col-md-4 down-btn-wp">
                  <a href="" class="down-btn">
                            Download
                        </a>
                </div>
              </div>
            </div>
          </div>
        </section>

        <!--about footer-->
        <%- include('./layouts/service') -%>
          <!--about footer end-->
          <%- include('./layouts/footer') -%>
